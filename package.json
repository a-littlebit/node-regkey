{
  "name": "regkey",
  "version": "2.3.2",
  "description": "Provide fast access to Windows registry for Node.js using native Windows API.",
  "keywords": [
    "registry",
    "Windows",
    "win32"
  ],
  "author": "vvcoder",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/a-littlebit/node-regkey.git"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "binding.gyp",
    "dist/index.cjs",
    "dist/index.mjs",
    "include",
    "index.d.ts",
    "LICENSE",
    "README.md",
    "src"
  ],
  "scripts": {
    "install": "node-gyp rebuild",
    "test": "node --trace-warnings ./tests/test.js",
    "clean": "node-gyp clean && npx rimraf dist",
    "rollup": "rollup -c",
    "build:debug": "node-gyp configure build --debug",
    "build:release": "node-gyp configure build --release"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "node-addon-api": "^8.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.29.1"
  }
}
