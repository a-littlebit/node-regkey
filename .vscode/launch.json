{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Test JavaScript",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/tests/test.js"
        },
        {
            "name": "Attach Native Process",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Debug Native Code",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "node",
            "args": ["${workspaceFolder}/tests/test.js"],
            "cwd": "${workspaceFolder}"
        }
    ]
}
